      <checkbox id="46" bindtap="select" style="position:fixed; left:20%; top:68%;">桌游</checkbox>
    </view>
  </swiper-item>
  <swiper-item>
    <view class="tip">
      <image src="../../assets/matchcenter/symbol.png" style="width:48rpx; height:48rpx;"></image>
      <text>人生</text>
    </view>
    <view class="tag">
      <checkbox id="47" bindtap="select" style="position:fixed; left:44%; top:46%;">文静</checkbox>
      <checkbox id="48" bindtap="select" style="position:fixed; left:67%; top:36%;">活泼</checkbox>
      <checkbox id="49" bindtap="select" style="position:fixed; left:8%; top:27%;">社恐</checkbox>
