    </view>
  </swiper-item>
  <swiper-item>
    <view class="tip">
      <image src="../../assets/matchcenter/clock.png" style="width:58rpx; height:56rpx;"></image>
      <text>生活</text>
    </view>
    <view class="tag">
      <checkbox id="14" bindtap="select" style="position:fixed; left:40%; top:56%;">早睡早起</checkbox>
