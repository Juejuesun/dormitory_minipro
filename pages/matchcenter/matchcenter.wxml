      <checkbox id="57" bindtap="select" style="position:fixed; left:8%; top:40%;">平淡是真</checkbox>
      <checkbox id="58" bindtap="select" style="position:fixed; left:8%; top:55%;">轰轰烈烈</checkbox>
    </view>
    <view class="quit-mes" wx:if="{{showQuitmes}}">
      <view class="control-create">
        <view class="accept" bindtap="matching">确认</view>
        <view bindtap="quitCreate" bindtap="changeQuit">取消</view>
      </view>
    </view>
    <button class='bottom' bindtap="match" style="width:60%;  color:#2F80ED;  background-color:white; border-radius: 40rpx; position:fixed; border:4rpx #2F80ED solid; left:20%; top:86%">开始匹配</button>
  </swiper-item>
</swiper>