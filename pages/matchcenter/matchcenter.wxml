      <checkbox id="0" class="tag1" bindtap="select" style="position:fixed; left:20%; top:44%;">考研党</checkbox>
      <checkbox id="1" bindtap="select" style="position:fixed; left:40%; top:56%;">毕业就打工人</checkbox>
      <checkbox id="2" bindtap="select" style="position:fixed; left:60%; top:46%;">自觉的读书人</checkbox>
      <checkbox id="3" bindtap="select" style="position:fixed; left:43%; top:34%;">自觉性差</checkbox>
      <checkbox id="4" bindtap="select" style="position:fixed; left:68%; top:80%;">相约读书</checkbox>
      <checkbox id="5" bindtap="select" style="position:fixed; left:14%; top:21%;">一人游书海</checkbox>
      <checkbox id="6" bindtap="select" style="position:fixed; left:40%; top:13%;">爱提问</checkbox>
      <checkbox id="7" bindtap="select" style="position:fixed; left:12%; top:78%;">独立解决</checkbox>
