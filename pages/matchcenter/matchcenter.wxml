      <checkbox id="21" bindtap="select" style="position:fixed; left:8%; top:78%;">DK</checkbox>
      <checkbox id="22" bindtap="select" style="position:fixed; left:4%; top:58%;">汉服</checkbox>
      <checkbox id="23" bindtap="select" style="position:fixed; left:70%; top:8%;">清淡</checkbox>
      <checkbox id="24" bindtap="select" style="position:fixed; left:2%; top:29%;">无辣不欢</checkbox>
      <checkbox id="25" bindtap="select" style="position:fixed; left:64%; top:26%;">火锅</checkbox>
      <checkbox id="26" bindtap="select" style="position:fixed; left:40%; top:79%;">烤肉</checkbox>
      <checkbox id="27" bindtap="select" style="position:fixed; left:18%; top:8%;">日料</checkbox>
      <checkbox id="28" bindtap="select" style="position:fixed; left:10%; top:19%;">韩料</checkbox>
      <checkbox id="29" bindtap="select" style="position:fixed; left:16%; top:68%;">动物真可爱</checkbox>
      <checkbox id="30" bindtap="select" style="position:fixed; left:60%; top:64%;">不喜欢动物</checkbox>
    </view>
  </swiper-item>
  <swiper-item>
    <view class="tip">
      <image src="../../assets/matchcenter/game.png" style="width:60rpx; height:44rpx;"></image>
      <text>兴趣</text>
    </view>
    <view class="tag">
      <checkbox id="31" bindtap="select" style="position:fixed; left:40%; top:56%;">王者荣耀</checkbox>
      <checkbox id="32" bindtap="select" style="position:fixed; left:67%; top:42%;">和平精英</checkbox>
